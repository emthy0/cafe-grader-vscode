{
  "name": "grader-scraper",
  "version": "0.1.0",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "engines": {
    "pnpm": ">=8.1.0",
    "node": ">=18"
  },
  "packageManager": "pnpm@8.1.0",
  "scripts": {
    "start": "pnpm run test:i",
    "dev": "npx nodemon src/index.ts",
    "build": "rm -rf ./dist/ && rollup -c",
    "test:i": "jest --watch --coverage",
    "test": "jest --coverage",
    "prepublish": "pnpm run test && pnpm run build",
    "commit": "git-cz",
    "setup:semantic-release": "npx semantic-release-cli setup"
  },
  "husky": {
    "pre-commit": "prettier src/* --write",
    "pre-push": "pnpm run test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/emthy0/grader-scraper"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^24.0.12",
    "@types/node": "16.x",
    "comment-json": "^1.1.3",
    "git-cz": "^3.0.1",
    "husky": "^2.2.0",
    "jest": "^24.8.0",
    "nodemon": "^2.0.22",
    "parcel-bundler": "^1.12.3",
    "prettier": "^1.17.0",
    "rollup": "^1.11.3",
    "rollup-plugin-typescript2": "^0.21.0",
    "semantic-release": "^15.13.3",
    "ts-jest": "^24.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^3.4.5"
  },
  "jest": {
    "preset": "ts-jest",
    "collectCoverageFrom": [
      "src/*.{ts,tsx}"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/build/",
      "/dist/"
    ]
  },
  "files": [
    "dist/"
  ],
  "description": "Node Scraper for Cafe-Grader",
  "dependencies": {
    "puppeteer": "^19.8.5"
  }
}

